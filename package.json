{
	"name": "jsontotype",
	"displayName": "jsonToType",
	"description": "easy to transform json to typescript type",
	"version": "0.0.5",
	"publisher": "tywin",
	"repository": {
		"url": "https://github.com/biglemontree/jsontotype.git"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Formatters",
		"Extension Packs",
		"Other"
	],
	"activationEvents": [
		"onCommand:jsonToType.toSelector",
		"onCommand:jsonToType.toType"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jsonToType.toSelector",
				"title": "selected json → selector function"
			},
			{
				"command": "jsonToType.toType",
				"title": "selected json → TypeScript Interface",
				"category": "jsonToType.toType"
			}
		],
		"keybindings": [
			{
				"command": "jsonToType.toType",
				"key": "ctrl+shift+`",
				"mac": "ctrl+shift+`",
				"when": "editorTextFocus"
			},
			{
				"command": "jsonToType.toType",
				"key": "ctrl+shift+i",
				"mac": "ctrl+shift+i",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "jsonToType.toType",
					"group": "5_cutcopypaste"
				},
				{
					"command": "jsonToType.toSelector",
					"group": "6_cutcopypaste"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"prettier": "^1.19.1",
		"quicktype-core": "^6.0.52",
		"quicktype-typescript-input": "0.0.11"
	}
}

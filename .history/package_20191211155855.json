{
	"name": "jsontotype",
	"displayName": "jsonToType",
	"description": "easy to transform json to typescript type",
  "version": "0.0.1",
  "publisher": "tywin",
  "repository": {},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
    "Formatters",
		"Extension Packs",
		"Other"
	],
	"activationEvents": [
    "onCommand:extension.helloWorld",
    "onCommand:jsontotype.totype"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "jsontotype.totype",
        "title": "selected json â†’ TypeScript Interface",
        "category": "jsontotype.totype"
			}
    ],
    "keybindings": [
			{
				"command": "jsontotype.totype",
				"key": "ctrl+shift+1",
				"mac": "ctrl+shift+1",
				"when": "editorTextFocus"
			}
		],
    "menus": {
      "editor/context": [
        {
          "command": "jsontotype.totype",
					"group": "5_cutcopypaste"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
